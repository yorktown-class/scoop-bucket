        "Microsoft Visual C++ Redistributables bundle": [
            "extra/vcredist2022",
            "extra/vcredist-aio",
            "extra/vcredist"
        ]
    },
    "depends": "extra/vcredist2022",
    "disable_checkver": {
        "url": "https://github.com/xiaoyaocz/dart_simple_live/releases",
        "regex": "v(?<ver>\\d*\\.\\d*\\.\\d*).*\\+(?<number>\\d*)-",
        "replace": "$matchVer"
    },
    "disable_autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/xiaoyaocz/dart_simple_live/releases/download/v$version/simple_live_app-$version+$matchNumber-windows.zip"
            }
        }
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/xiaoyaocz/dart_simple_live/releases/download/v1.4.8/simple_live_app-1.4.8+10408-windows.zip",
            "hash": "B7CDFD326634DFAD49EB0676065EA1913377DC4314886A513D89B0660B5AF4A9"
        }
    },
    "shortcuts": [
        [
            "simple_live_app.exe",
            "聚合直播"
        ]
    ]
